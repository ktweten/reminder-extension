{
    "name": "__MSG_appName__",
    "version": "1.0.28",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/Note16.png",
        "128": "images/Note.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/update-icon.js",
            "scripts/chromereload.js",
            "scripts/background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "19": "images/Note19.png",
            "38": "images/Note38.png"
        },
        "default_title": "Reminder",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "alarms",
        "storage"
    ]
}